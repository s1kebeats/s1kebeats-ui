{
  "name": "@s1kebeats/s1kebeats-ui",
  "version": "3.4.0",
  "author": "Arthur Datsenko-Boos",
  "description": "@s1kebeats 2023",
  "files": [
    "dist"
  ],
  "types": "./dist/index.d.ts",
  "main": "./dist/s1kebeats-ui.umd.cjs",
  "module": "./dist/s1kebeats-ui.mjs",
  "exports": {
    ".": {
      "import": "./dist/s1kebeats-ui.mjs",
      "require": "./dist/s1kebeats-ui.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "storybook": "conc \"pnpm run format\" \"storybook dev -p 6006\" \"pnpm run dev-tailwind\"",
    "build-storybook": "storybook build -o docs",
    "build-tailwind": "pnpm dlx tailwindcss -o .storybook/tailwind.css",
    "dev-tailwind": "pnpm dlx tailwindcss -i ./src/assets/main.css -o .storybook/tailwind.css --watch",
    "build-publish": "pnpm run build && npm publish"
  },
  "dependencies": {
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@iconify/vue": "^4.1.1",
    "@rushstack/eslint-patch": "^1.2.0",
    "@storybook/addon-essentials": "7.0.25",
    "@storybook/addon-interactions": "7.0.25",
    "@storybook/addon-links": "7.0.25",
    "@storybook/addon-styling": "^1.3.2",
    "@storybook/blocks": "7.0.25",
    "@storybook/testing-library": "0.0.14-next.2",
    "@storybook/vue3": "7.0.25",
    "@storybook/vue3-vite": "7.0.25",
    "@tsconfig/node18": "^2.0.1",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^18.16.17",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.3.2",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.14",
    "cross-env": "^7.0.3",
    "eslint": "^8.39.0",
    "eslint-plugin-storybook": "^0.6.12",
    "eslint-plugin-vue": "^9.11.0",
    "jsdom": "^22.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.24",
    "postcss-cli": "^10.1.0",
    "prettier": "^2.8.8",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "^1.63.6",
    "storybook": "7.0.25",
    "tailwindcss": "^3.3.2",
    "typescript": "~5.0.4",
    "vite": "^4.3.9",
    "vite-plugin-dts": "^3.3.1",
    "vitest": "^0.32.0",
    "vue-tsc": "^1.6.5"
  }
}
